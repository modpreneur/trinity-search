{
  "name": "trinity/search",
  "description": "Trinity Search bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Trinity Team",
      "email": "info@modpreneur.com"
    }
  ],

  "require": {
    "php": ">=7",
    "symfony/framework-bundle": "~2.8 | ~3.0",
    "nette/utils": "~2.3",
    "trinity/framework": "dev-master",
    "friendsofsymfony/rest-bundle": "~1.7",
    "sensio/framework-extra-bundle": "~3.0",
    "jms/serializer-bundle": "~1.1",
    "friendsofsymfony/user-bundle": "master-dev",
    "trinity/settings": "dev-master",
    "trinity/logger": "dev-master"
  },

  "require-dev": {
    "symfony/swiftmailer-bundle": "~2.3",
    "sensio/generator-bundle": "~3.0",
    "phpunit/phpunit": "5.1.*",
    "mockery/mockery": "~0.9",
    "symfony/var-dumper": "~3.0",
    "fzaninotto/faker": "^1.5"
  },

  "autoload": {
    "psr-4": { "Trinity\\Bundle\\SearchBundle\\": "" },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  },



  "minimum-stability": "stable",
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
